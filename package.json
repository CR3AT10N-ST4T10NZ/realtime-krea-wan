    "@vercel/analytics": "^1.5.0",
    "botid": "^1.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "konva": "^10.0.2",
    "lucide-react": "^0.525.0",
    "next": "^15.2.0",
